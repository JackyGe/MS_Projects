  <DataSets>
    <DataSet Name="DS_Userinfo">
      <Query>
        <DataSourceName>REF_SDS_Attendance</DataSourceName>
        <CommandText>select User_ID, dbo.User_View.Name AS 'UserName', DeptName, Group_Name, dbo.User_View.SOSA_Account, dbo.Userinfo.Resigned
from dbo.User_View left join dbo.Userinfo
on dbo.User_View.Userid = dbo.Userinfo.Userid
where dbo.Userinfo.Resigned&lt;&gt;1
order by User_ID</CommandText>
      </Query>
      <Fields>
        <Field Name="User_ID">
          <DataField>User_ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserName">
