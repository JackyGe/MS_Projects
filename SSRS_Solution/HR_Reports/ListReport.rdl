        </Field>
        <Field Name="Group_Name">
          <DataField>Group_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SOSA_Account">
          <DataField>SOSA_Account</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Resigned">
          <DataField>Resigned</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
