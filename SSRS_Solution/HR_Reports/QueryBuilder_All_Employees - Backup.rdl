        </Field>
      </Fields>
      <Query>
        <DataSourceName>SDS_Attendance</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        Dept.Deptid, Dept.DeptName, Userinfo.Userid, Userinfo.Name, Userinfo.Name_EN,  Userinfo.Resigned
FROM            Dept RIGHT OUTER JOIN
                         Userinfo ON Dept.Deptid = Userinfo.Deptid
WHERE        (Userinfo.Resigned = 0)</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
