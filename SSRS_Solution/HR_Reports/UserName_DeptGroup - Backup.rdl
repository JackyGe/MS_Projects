        </Field>
      </Fields>
      <Query>
        <DataSourceName>SDS_Attendance</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select User_ID, dbo.User_View.Name AS 'UserName', DeptName, Group_Name, dbo.User_View.SOSA_Account
from dbo.User_View left join dbo.Userinfo
on dbo.User_View.Userid = dbo.Userinfo.Userid
where dbo.Userinfo.Resigned&lt;&gt;1
order by User_ID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
